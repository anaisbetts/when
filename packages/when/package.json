{
  "name": "@whenjs/when",
  "version": "0.2.1",
  "scripts": {
    "compile": "git clean -xdf ./lib && \"../../node_modules/.bin/tsc\" -p . --outDir ./lib",
    "lint": "echo '' && \"../../node_modules/.bin/tslint\" -c ./tslint.json -p ./tsconfig.json \"src/**/*.ts\"",
    "prepublish": "npm run compile",
    "test": "npm run test-node && npm run lint",
    "test-node": "echo '' && \"../../node_modules/.bin/mocha\" --compilers ts:ts-node/register \"./test/**/*-spec.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulcbetts/when.git"
  },
  "author": "Paul Betts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/paulcbetts/when/issues"
  },
  "homepage": "https://github.com/paulcbetts/when#readme",
  "main": "lib/src/index.js",
  "typings": "lib/src/index.d.ts",
  "dependencies": {
    "debug": "3.1.0",
    "deepmerge": "^2.1.1",
    "lodash.isequal": "4.5.0",
    "lodash.isfunction": "3.0.9",
    "lodash.isobject": "3.0.2",
    "lru-cache": "^4.1.3",
    "rxjs": "^6.2.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
